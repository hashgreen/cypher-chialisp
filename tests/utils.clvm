(
  (defmacro tests.utils.expect-failed (statement)
    (qq (if
      (= (f (mod () (unquote statement))) (q . x))
      1
      (x
        (q . (unquote statement))
        "should fail!"
      )
    ))
  )

  (defmacro tests.utils.expect-equal (statement result)
    (qq (if
      (= (unquote statement) (unquote result))
      1
      (x
        (q . (unquote statement))
        "does not evaluate to"
        (q . (unquote result))
      )
    ))
  )
)
