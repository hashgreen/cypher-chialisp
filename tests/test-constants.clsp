(a (mod
  ()

  (include cypher/constants.clsp)

  (include tests/utils.clsp)

  (defun-inline test-constants ()
    (@assert
      (@assert-equal cf.NIL 0)
      (@assert-equal cf.ZERO 0)
      (@assert-equal cf.ONE 1)
      (@assert-equal cf.TWO 2)
      (@assert-equal cf.ONE_2 100)
      (@assert-equal cf.ONE_3 1000)
      (@assert-equal cf.ONE_6 1000000)
      (@assert-equal cf.ONE_9 1000000000)
      (@assert-equal cf.ONE_12 1000000000000)
      "test-constants"))

  (test-constants)

) ())
