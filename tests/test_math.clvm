(a (mod ()

  (include cypher/macros.clvm)
  (include cypher/math.clvm)

  (defun test-sqrt ()
    (cf.ASSERT
      (= (cf.sqrt 0) 0)
      (= (cf.sqrt 1) 1)
      (= (cf.sqrt 2) 1)
      (= (cf.sqrt 3) 1)
      (= (cf.sqrt 4) 2)
      (= (cf.sqrt 8) 2)
      (= (cf.sqrt 100) 10)
      (= (cf.sqrt 420) 20)
      (= (cf.sqrt 10000) 100)
      (= (cf.sqrt 1000000000000) 1000000)
    )
  )

  (defun test-round ()
    (cf.ASSERT
      (= (cf.round-even-odd 0 0 0) 1)
      (= (cf.round-even-odd 1 0 0) 1)
      (= (cf.round-even-odd 2 0 0) 3)
      (= (cf.round-even-odd 3 0 0) 3)
      (= (cf.round-even-odd 0 1 0) 0)
      (= (cf.round-even-odd 1 1 0) 2)
      (= (cf.round-even-odd 2 1 0) 2)
      (= (cf.round-even-odd 3 1 0) 4)
      (= (cf.round-even-odd 0 0 1) -1)
      (= (cf.round-even-odd 1 0 1) 1)
      (= (cf.round-even-odd 2 0 1) 1)
      (= (cf.round-even-odd 3 0 1) 3)
      (= (cf.round-even-odd 0 1 1) 0)
      (= (cf.round-even-odd 1 1 1) 0)
      (= (cf.round-even-odd 2 1 1) 2)
      (= (cf.round-even-odd 3 1 1) 2)
    )
  )

  (cf.CONCAT
    (test_sqrt)
    (test_round)
  )
) ())
