(a (mod ()

  (include cypher/macros.clvm)
  (include cypher/math.clvm)

  (include tests/utils.clvm)

  (defun-inline test-unsigned-sub ()
    (@cf-tests.assert
      (@cf-tests.assert-equal (cf.unsigned-sub 2 2) 0)
      (@cf-tests.assert-fails (cf.unsigned-sub 1 2))
      "test-math.test-unsigned-sub"))

  (defun-inline test-round ()
    (@cf-tests.assert
      (@cf-tests.assert-equal (@cf.round-even-odd 0 0 0) 1)
      (@cf-tests.assert-equal (@cf.round-even-odd 1 0 0) 1)
      (@cf-tests.assert-equal (@cf.round-even-odd 2 0 0) 3)
      (@cf-tests.assert-equal (@cf.round-even-odd 3 0 0) 3)
      (@cf-tests.assert-equal (@cf.round-even-odd 0 1 0) 0)
      (@cf-tests.assert-equal (@cf.round-even-odd 1 1 0) 2)
      (@cf-tests.assert-equal (@cf.round-even-odd 2 1 0) 2)
      (@cf-tests.assert-equal (@cf.round-even-odd 3 1 0) 4)
      (@cf-tests.assert-equal (@cf.round-even-odd 0 0 1) -1)
      (@cf-tests.assert-equal (@cf.round-even-odd 1 0 1) 1)
      (@cf-tests.assert-equal (@cf.round-even-odd 2 0 1) 1)
      (@cf-tests.assert-equal (@cf.round-even-odd 3 0 1) 3)
      (@cf-tests.assert-equal (@cf.round-even-odd 0 1 1) 0)
      (@cf-tests.assert-equal (@cf.round-even-odd 1 1 1) 0)
      (@cf-tests.assert-equal (@cf.round-even-odd 2 1 1) 2)
      (@cf-tests.assert-equal (@cf.round-even-odd 3 1 1) 2)
      "test-math.test-round"))

  (defun-inline test-sqrt ()
    (@cf-tests.assert
      (@cf-tests.assert-equal (cf.sqrt 0) 0)
      (@cf-tests.assert-equal (cf.sqrt 1) 1)
      (@cf-tests.assert-equal (cf.sqrt 2) 1)
      (@cf-tests.assert-equal (cf.sqrt 3) 1)
      (@cf-tests.assert-equal (cf.sqrt 4) 2)
      (@cf-tests.assert-equal (cf.sqrt 8) 2)
      (@cf-tests.assert-equal (cf.sqrt 100) 10)
      (@cf-tests.assert-equal (cf.sqrt 420) 20)
      (@cf-tests.assert-equal (cf.sqrt 10000) 100)
      (@cf-tests.assert-equal (cf.sqrt 1000000000000) 1000000)
      "test-math.test-sqrt"))

  (defun-inline test-math ()
    (@cf-tests.assert
      (test-unsigned-sub)
      (test-round)
      (test-sqrt)
      "test-math"))

  (test-math)

) ())
