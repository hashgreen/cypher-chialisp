(a (mod ()

  (include cypher/macros.clvm)
  (include cypher/math.clvm)

  (include tests/utils.clvm)

  (defun-inline test-unsigned-sub ()
    (tests.utils.assert
      (tests.utils.assert-equal (cypher.math.unsigned-sub 2 2) 0)
      (tests.utils.assert-fails (cypher.math.unsigned-sub 1 2))
      "test-math.test-unsigned-sub"))

  (defun-inline test-round ()
    (tests.utils.assert
      (tests.utils.assert-equal (cypher.math.round-even-odd 0 0 0) 1)
      (tests.utils.assert-equal (cypher.math.round-even-odd 1 0 0) 1)
      (tests.utils.assert-equal (cypher.math.round-even-odd 2 0 0) 3)
      (tests.utils.assert-equal (cypher.math.round-even-odd 3 0 0) 3)
      (tests.utils.assert-equal (cypher.math.round-even-odd 0 1 0) 0)
      (tests.utils.assert-equal (cypher.math.round-even-odd 1 1 0) 2)
      (tests.utils.assert-equal (cypher.math.round-even-odd 2 1 0) 2)
      (tests.utils.assert-equal (cypher.math.round-even-odd 3 1 0) 4)
      (tests.utils.assert-equal (cypher.math.round-even-odd 0 0 1) -1)
      (tests.utils.assert-equal (cypher.math.round-even-odd 1 0 1) 1)
      (tests.utils.assert-equal (cypher.math.round-even-odd 2 0 1) 1)
      (tests.utils.assert-equal (cypher.math.round-even-odd 3 0 1) 3)
      (tests.utils.assert-equal (cypher.math.round-even-odd 0 1 1) 0)
      (tests.utils.assert-equal (cypher.math.round-even-odd 1 1 1) 0)
      (tests.utils.assert-equal (cypher.math.round-even-odd 2 1 1) 2)
      (tests.utils.assert-equal (cypher.math.round-even-odd 3 1 1) 2)
      "test-math.test-round"))

  (defun-inline test-sqrt ()
    (tests.utils.assert
      (tests.utils.assert-equal (cypher.math.sqrt 0) 0)
      (tests.utils.assert-equal (cypher.math.sqrt 1) 1)
      (tests.utils.assert-equal (cypher.math.sqrt 2) 1)
      (tests.utils.assert-equal (cypher.math.sqrt 3) 1)
      (tests.utils.assert-equal (cypher.math.sqrt 4) 2)
      (tests.utils.assert-equal (cypher.math.sqrt 8) 2)
      (tests.utils.assert-equal (cypher.math.sqrt 100) 10)
      (tests.utils.assert-equal (cypher.math.sqrt 420) 20)
      (tests.utils.assert-equal (cypher.math.sqrt 10000) 100)
      (tests.utils.assert-equal (cypher.math.sqrt 1000000000000) 1000000)
      "test-math.test-sqrt"))

  (defun-inline test-math ()
    (tests.utils.assert
      (test-unsigned-sub)
      (test-round)
      (test-sqrt)
      "test-math"))

  (test-math)

) ())
